{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { 
      "source": "/api/:path*", 
      "destination": "https://resume-ai-backend.vercel.app/api/:path*" 
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
