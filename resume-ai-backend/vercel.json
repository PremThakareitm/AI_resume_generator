{
  "version": 2,
  "buildCommand": "./build.sh",
  "outputDirectory": "target",
  "framework": "other",
  "builds": [
    {
      "src": "target/*.jar",
      "use": "@vercel/java"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "target/resume-ai-backend-0.0.1-SNAPSHOT.jar"
    },
    {
      "src": "/(.*)",
      "dest": "target/resume-ai-backend-0.0.1-SNAPSHOT.jar"
    }
  ],
  "env": {
    "SPRING_PROFILES_ACTIVE": "production"
  },
  "regions": ["sfo1"]
}